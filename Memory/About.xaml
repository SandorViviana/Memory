<Window x:Class="Memory.About"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Memory"
        mc:Ignorable="d"
        Title="About" Height="250" Width="400" Background="AntiqueWhite">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="1*" />
            <RowDefinition Height="1*" />
            <RowDefinition Height="1*" />
            <RowDefinition Height="1*" />
        </Grid.RowDefinitions>
        <TextBlock Grid.Row="0" FontFamily="Georgia" FontSize="20" VerticalAlignment="Center" HorizontalAlignment="Center" Margin="10">This game was brought to you by</TextBlock>
        <TextBlock Grid.Row="1" FontFamily="Georgia" FontSize="20" VerticalAlignment="Center"  HorizontalAlignment="Center" Margin="10">Sandor Viviana</TextBlock>
        <TextBlock Grid.Row="2" FontFamily="Georgia" FontSize="20" VerticalAlignment="Center"  HorizontalAlignment="Center" Margin="10">10LF213</TextBlock>
        <TextBlock Grid.Row="3" FontFamily="Georgia" FontSize="20" VerticalAlignment="Center"  HorizontalAlignment="Center" Margin="10">Computer Science</TextBlock>
    </Grid>
    
</Window>
